{
    "extends": "./tsconfig.build.json",
    "compilerOptions": {
        "alwaysStrict": true,
        "declaration": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "sourceMap": true,
        "strictBindCallApply": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
    },
}
